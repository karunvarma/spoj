#include<bits/stdc++.h>
using namespace std;
#define ll				long long int


int main()
{

    int t;
    cin>>t;

    while(t--)
    {
        string s;
        cin>>s;
        int k=0,ke=0,kek=0;

        for(int i=0;i<s.size();i++)
        {
            if(s[i]=='K')
            {
                k++;
                kek+=(ke);
            }
            if(s[i]=='E')
            {
                ke+=k;
            }
        }
        cout<<kek<<endl;

    }
    return 0;

}
